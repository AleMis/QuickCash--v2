<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="pl.my.quickcash.controllers.employee.ClientsDatabasePanel2Controller">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <children>
        <SplitPane dividerPositions="0.11371237458193979" orientation="VERTICAL" prefHeight="600.0" prefWidth="1200.0">
            <items>
                <Pane prefHeight="106.0" prefWidth="1198.0">
                    <children>
                        <TextField layoutX="379.0" layoutY="20.0" promptText="pesel" />
                        <Button layoutX="547.0" layoutY="20.0" mnemonicParsing="false" text="Button" />
                        <Label layoutX="282.0" layoutY="24.0" text="Search by Pesel:" />
                        <Button layoutX="76.0" layoutY="20.0" mnemonicParsing="false" text="Load Clients Database" />
                    </children>
                </Pane>
                <Pane>
                    <children>
                        <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1">
                            <columns>
                                <TableColumn fx:id="loginColumn" text = "Login" prefWidth="100">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="login" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="passwordColumn" text = "Password" prefWidth="100">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="password" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="firstNameColumn" text = "First Name" prefWidth="100">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="firstName" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="lastNameColumn" text = "Last Name" prefWidth="100">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="lastName" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="countryColumn" text = "Country" prefWidth="100">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="country" />
                                    </cellValueFactory>
                                </TableColumn>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                            <sortOrder>
                                <fx:reference source="loginColumn">
                                </fx:reference>
                                <fx:reference source="firstNameColumn">
                                </fx:reference>
                                <fx:reference source="lastNameColumn">
                                </fx:reference>
                                <fx:reference source="countryColumn">
                                </fx:reference>
                            </sortOrder>
                        </TableView>
                    </children>
                </Pane>
            </items>
        </SplitPane>
    </children>
</GridPane>
